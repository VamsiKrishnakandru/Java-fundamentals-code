import java.util.Arrays;
import java.util.List;

public class LaunchMethodRef {

	public static void main(String[] args) 
	{
		List<String> courses = Arrays.asList("Java", "SpringBoot", "SpringAI", "Microservices");
		List<String> uCourses = courses.stream().map(course->course.toUpperCase()).toList();
		//method reference
		List<String> uCourses1 = courses.stream().map(String::toUpperCase).toList();
		System.out.println(uCourses);
	}

}
