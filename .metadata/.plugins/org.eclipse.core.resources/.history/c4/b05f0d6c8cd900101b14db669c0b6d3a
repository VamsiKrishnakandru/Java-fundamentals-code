import java.util.ArrayList;

public class LaunchAccessingData {

	public static void main(String[] args) 
	{
		ArrayList<E> al = new ArrayList();
		
		al.add(100);
		al.add(200);
		al.add(300);
		al.add(400);
		
		Integer iData = (Integer) al.get(2); // data stored is in object form so we need to downcast object to integer.
		System.out.println(iData); //index based accessing only allowed in arrayList and LinkedList
		
		for(int i=0; i<al.size(); i++)
		{
			System.out.println(al.get(i));
		}
		
	}

}
