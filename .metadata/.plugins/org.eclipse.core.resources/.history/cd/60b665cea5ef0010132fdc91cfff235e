abstract class SamplePlane
{
	static String name;
	static
	{
		name="Boeing";
	}
	abstract void planeAltitude();
	abstract void planeDuration();
	public void planeStatus()
	{
		System.out.println("Not Applicable");
	}
	static void display()
	{
		System.out.println("Parent class static method");
	}
}
class SamplePlane1 extends SamplePlane
{
	void planeAltitude()
	{
		System.out.println(name + " Plane1 is flying at high altitude");
	}
	void planeDuration()
	{
		System.out.println(name + " Plane1 has medium duration");
	}
	public void planeStatus()
	{
		System.out.println("At destination");
	}
	static void display()
	{
		System.out.println("Child class static method");
	}
}
class SamplePlane2 extends SamplePlane
{
	void planeAltitude()
	{
		System.out.println(name + " Plane2 is flying at low altitude");
	}
	void planeDuration()
	{
		System.out.println(name + " Plane2 has long duration");
	}
}
class SamplePlane3 extends SamplePlane
{
	void planeAltitude()
	{
		System.out.println(name + " Plane3 is flying at medium altitude");
	}
	void planeDuration()
	{
		System.out.println(name + " Plane3 has short duration");
	}
}
class SampleAirport
{
	public void show(SamplePlane ref)
	{
		ref.planeAltitude();
		ref.planeDuration();
	}
}
public class SamplePolymorphism {
	public static void main(String[] args) 
	{
		SamplePlane p1 = new SamplePlane1();
		SamplePlane p2 = new SamplePlane2();
		SamplePlane p3 = new SamplePlane3();
		SampleAirport a1 = new SampleAirport();
		a1.show(p1);
		a1.show(p2);
		a1.show(p3);
		p1.display();
		((SamplePlane1) p1).display();
		SamplePlane.display();
		SamplePlane1.display();
	}
}
