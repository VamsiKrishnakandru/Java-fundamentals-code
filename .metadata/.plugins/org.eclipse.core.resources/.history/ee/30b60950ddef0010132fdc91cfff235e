
public class LaunchBubbleSort {

	public static void main(String[] args) {
		int []array = {2,5,3,9,1,4,10,8,7};
		int size=array.length;
		int temp=0;
		for(int ele:array)
		{
			System.out.print(ele + " ");
		}
		for(int i=0;i<size;i++)
		{
			for(int j=0;j<size-j-1;j++)
			{
				if(array[j]>array[j+1])
				{
					temp=array[j];
					array[j]=array[j+1];
					array[j+1]=temp;
				}
			}
		}
		System.out.println();
		for(int ele:array)
		{
			System.out.print(ele + " ");
		}
	}
}
