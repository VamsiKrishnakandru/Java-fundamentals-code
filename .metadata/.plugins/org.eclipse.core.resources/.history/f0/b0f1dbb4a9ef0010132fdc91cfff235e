class SampleFlipkartApp
{
	SampleDeliveryPartner ref;
	public SampleFlipkartApp(SampleDeliveryPartner ref)
	{
		System.out.println("Dependency injection using constructor");
		this.ref=ref;
	}
	public void setDeliveryPartner(SampleDeliveryPartner ref)
	{
		System.out.println("Dependency injection using setter");
		this.ref=ref;
	}
	public void display()
	{
		ref.showDetails();
	}
}
interface SampleDeliveryPartner
{
	void showDetails();
}
class Dtdc implements SampleDeliveryPartner
{
	public void showDetails()
	{
		System.out.println("Dtdc has delivered your courier");
	}
}
class BlueDart implements SampleDeliveryPartner
{
	public void showDetails()
	{
		System.out.println("BlueDart has delivered your courier");
	}
}
public class SampleLosseCoupling {

	public static void main(String[] args) 
	{
		SampleFlipkartApp s1 = new SampleFlipkartApp(new Dtdc());
		s1.display();
	}
}
